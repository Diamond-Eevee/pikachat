{
    "compilerOptions": {
      "target": "ES5",
      "module": "CommonJS",
      "outDir": "out",
      "sourceMap": true,      
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmitOnError": false, // Allow compilation even if there are type errors
      "noImplicitAny": false
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  